<Page x:Class="TRPO.Pages.DataPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:TRPO.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="DataPage">

    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <Button x:Name="UploadButton" Content="Выгрузить" Margin="15" Padding="5" Click="UploadButton_Click"/>
            <Button x:Name="DownloadButton" Content="Загрузить" Margin="15" Padding="5" Click="DownloadButton_Click"/>
        </StackPanel>
    </Grid>
</Page>
